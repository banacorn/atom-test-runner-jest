{
  "name": "atom-test-runner-jest",
  "version": "0.2.3",
  "description": "Atom Test Runner for Jest",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keplersj/atom-test-runner-jest.git"
  },
  "keywords": [
    "atom",
    "jest"
  ],
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keplersj/atom-test-runner-jest/issues"
  },
  "homepage": "https://github.com/keplersj/atom-test-runner-jest#readme",
  "dependencies": {
    "jest-cli": "^22.4.3",
    "pkg-up": "^2.0.0",
    "tmp": "^0.0.33"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/is-ci": "^1.1.0",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.4",
    "execa": "^0.10.0",
    "is-ci": "^1.1.0",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/__fixtures__/",
      "/atom/"
    ]
  }
}
